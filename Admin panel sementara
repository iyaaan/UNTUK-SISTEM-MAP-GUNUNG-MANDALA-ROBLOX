	"ResetActionsButton",
	"Reset Aksi",
	UDim2.new(0, 90, 0, 25),
	UDim2.new(1, -95, 1, -30),
	Color3.fromRGB(200, 100, 60),
	Color3.fromRGB(255, 255, 255)
)
resetActionsButton.Parent = row3

-- Variabel state
local currentTarget = nil
local isAllPlayers = false

-- Fungsi animasi close dengan efek shrink ke tengah
local function closePanelWithAnimation()
	local currentPosition = mainPanel.Position
	local currentSize = mainPanel.Size
	
	-- Animasi mengecil ke tengah
	local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In)
	local tween1 = TweenService:Create(mainPanel, tweenInfo, {
		Size = UDim2.new(0, 0, 0, 0),
		Position = UDim2.new(0.5, 0, 0.5, 0)
	})
	
	-- Simultaneously fade out
	local tween2 = TweenService:Create(mainPanel, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		BackgroundTransparency = 1
	})
	
	tween1:Play()
	tween2:Play()
	
	tween1.Completed:Connect(function()
		mainPanel.Visible = false
		mainPanel.BackgroundTransparency = 0
		mainPanel.Size = originalSize
		mainPanel.Position = UDim2.new(0.5, -300, 0.5, -300)
		adminButton.Visible = true
		isMinimized = false
		minimizeButton.Text = "_"
		contentContainer.Visible = true
	end)
end

-- Fungsi untuk toggle minimize
local function toggleMinimize()
	isMinimized = not isMinimized
	
	if isMinimized then
		-- Minimize: sembunyikan content, kecilkan panel
		contentContainer.Visible = false
		minimizeButton.Text = "□"
		
		local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out)
		local tween = TweenService:Create(mainPanel, tweenInfo, {
			Size = minimizedSize
		})
		tween:Play()
		
		-- Non-aktifkan interaksi pada content
		for _, button in ipairs(actionButtons) do
			button.Active = false
		end
		applySpecificButton.Active = false
		applyAllButton.Active = false
		resetActionsButton.Active = false
		resetTargetButton.Active = false
		allPlayersButton.Active = false
		playerInput.Active = false
		
	else
		-- Maximize: tampilkan content, kembalikan ukuran panel
		minimizeButton.Text = "_"
		
		local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out)
		local tween = TweenService:Create(mainPanel, tweenInfo, {
			Size = originalSize
		})
		tween:Play()
		
		-- Tunggu animasi selesai baru tampilkan content
		tween.Completed:Connect(function()
			contentContainer.Visible = true
			
			-- Aktifkan kembali interaksi pada content
			for _, button in ipairs(actionButtons) do
				button.Active = true
			end
			applySpecificButton.Active = true
			applyAllButton.Active = true
			resetActionsButton.Active = true
			resetTargetButton.Active = true
			allPlayersButton.Active = true
			playerInput.Active = true
		end)
	end
end

-- Fungsi untuk open panel dengan animasi
local function openPanelWithAnimation()
	mainPanel.Visible = true
	adminButton.Visible = false
	mainPanel.Size = UDim2.new(0, 0, 0, 0)
	mainPanel.Position = UDim2.new(0.5, 0, 0.5, 0)
	
	local tweenInfo = TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out)
	local tween = TweenService:Create(mainPanel, tweenInfo, {
		Size = originalSize,
		Position = UDim2.new(0.5, -300, 0.5, -300)
	})
	tween:Play()
	
	-- Pastikan content visible saat panel dibuka
	contentContainer.Visible = true
	isMinimized = false
	minimizeButton.Text = "_"
end

-- Event handlers
adminButton.MouseButton1Click:Connect(openPanelWithAnimation)
minimizeButton.MouseButton1Click:Connect(toggleMinimize)
closeButton.MouseButton1Click:Connect(closePanelWithAnimation)

-- Reset Target
resetTargetButton.MouseButton1Click:Connect(function()
	if not isMinimized then
		playerInput.Text = ""
		currentTarget = nil
		isAllPlayers = false
		allPlayersButton.BackgroundColor3 = Color3.fromRGB(80, 80, 90)
		print("Target telah direset")
	end
end)

-- Target Semua Player
allPlayersButton.MouseButton1Click:Connect(function()
	if not isMinimized then
		isAllPlayers = not isAllPlayers
		if isAllPlayers then
			allPlayersButton.BackgroundColor3 = Color3.fromRGB(255, 165, 0)
			currentTarget = nil
			playerInput.Text = "SEMUA PLAYER"
		else
			allPlayersButton.BackgroundColor3 = Color3.fromRGB(80, 80, 90)
			playerInput.Text = ""
		end
	end
end)

-- Reset Pilihan Aksi
resetActionsButton.MouseButton1Click:Connect(function()
	if not isMinimized then
		for i, button in ipairs(actionButtons) do
			button.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
		end
		print("Semua pilihan aksi telah direset")
	end
end)

-- Terapkan ke Player Tertentu
applySpecificButton.MouseButton1Click:Connect(function()
	if not isMinimized then
		local selectedActions = {}
		for i, button in ipairs(actionButtons) do
			if button.BackgroundColor3 == Color3.fromRGB(0, 150, 255) then
				table.insert(selectedActions, i)
			end
		end
		
		if #selectedActions > 0 then
			local targetName = playerInput.Text
			if targetName ~= "" and targetName ~= "SEMUA PLAYER" then
				print("Menerapkan aksi " .. table.concat(selectedActions, ", ") .. " ke player: " .. targetName)
			else
				print("Pilih target player terlebih dahulu!")
			end
		else
			print("Pilih minimal satu aksi!")
		end
	end
end)

-- Terapkan ke Semua Player
applyAllButton.MouseButton1Click:Connect(function()
	if not isMinimized then
		local selectedActions = {}
		for i, button in ipairs(actionButtons) do
			if button.BackgroundColor3 == Color3.fromRGB(0, 150, 255) then
				table.insert(selectedActions, i)
			end
		end
		
		if #selectedActions > 0 then
			print("Menerapkan aksi " .. table.concat(selectedActions, ", ") .. " ke SEMUA PLAYER")
		else
			print("Pilih minimal satu aksi!")
		end
	end
end)

-- Tambahkan efek hover pada header untuk indikasi draggable
header.MouseEnter:Connect(function()
	local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local tween = TweenService:Create(header, tweenInfo, {BackgroundColor3 = Color3.fromRGB(50, 50, 55)})
	tween:Play()
end)

header.MouseLeave:Connect(function()
	if not dragging then
		local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(header, tweenInfo, {BackgroundColor3 = Color3.fromRGB(40, 40, 45)})
		tween:Play()
	end
end)

-- Efek hover untuk tombol minimize
minimizeButton.MouseEnter:Connect(function()
	local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local tween = TweenService:Create(minimizeButton, tweenInfo, {BackgroundColor3 = Color3.fromRGB(120, 120, 180)})
	tween:Play()
end)

minimizeButton.MouseLeave:Connect(function()
	local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local tween = TweenService:Create(minimizeButton, tweenInfo, {BackgroundColor3 = Color3.fromRGB(100, 100, 150)})
	tween:Play()
end)

-- Efek hover untuk tombol close
closeButton.MouseEnter:Connect(function()
	local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local tween = TweenService:Create(closeButton, tweenInfo, {
		BackgroundColor3 = Color3.fromRGB(220, 80, 80),
		TextColor3 = Color3.fromRGB(255, 255, 255)
	})
	tween:Play()
end)

closeButton.MouseLeave:Connect(function()
	local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	local tween = TweenService:Create(closeButton, tweenInfo, {
		BackgroundColor3 = Color3.fromRGB(200, 60, 60),
		TextColor3 = Color3.fromRGB(255, 255, 255)
	})
	tween:Play()
end)

print("Admin Panel berhasil dimuat! Klik tombol 'Admin' di sudut kanan atas untuk membuka.")
